<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yicheng.tourism.mapper.ext.MealPriceMapperExt">

    <insert id="insertBatch" parameterType="list">
        INSERT INTO t_meal_price
        (id, commodity_id,set_meal,price)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.id,jdbcType=VARCHAR}, #{item.commodityId,jdbcType=VARCHAR}, #{item.setMeal,jdbcType=VARCHAR},
            #{item.price,jdbcType=DOUBLE})
        </foreach>
    </insert>

</mapper>